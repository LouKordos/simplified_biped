<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <model name="biped">
      <include>
        <uri>model://simplified_biped</uri>
      </include>

      <plugin name="biped_plugin" filename="libbiped_plugin.so"/>
    </model>
  </world>
</sdf>
