<?xml version="1.0" ?>
<sdf version="1.5">

<model name="simplified_biped">

    <!--Left leg-->

    <link name="left_foot">
        <self_collide>1</self_collide>

        <pose> 0.1 0 0.03 0 0 0 </pose>

        <inertial>
            <mass> 0.01 </mass>
            <inertia>
                <ixx>0.0000045</ixx>
                <iyy>0.0000045</iyy>
                <izz>0.0000045</izz>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyz>0</iyz>
            </inertia>
        </inertial>

        <collision name="left_foot_collision">
            <geometry>
                <sphere>
                    <radius> 0.03 </radius>
                </sphere>
            </geometry>
        </collision>

        <visual name="left_foot_visual">
            <geometry>
                <sphere>
                    <radius> 0.03 </radius>
                </sphere>
            </geometry>
        </visual>
    </link>

    <link name="left_lower_leg">
        <self_collide>1 </self_collide>
        <!--Height should be foot diameter 
            + lower leg link length / 2-->
        <pose> 0.1 0 0.21 0 0 0 </pose>

        <inertial>
            <mass> 0.05 </mass>
            <inertia>
                <ixx>0.00038</ixx>
                <iyy>0.00038</iyy>
                <izz>0.000014</izz>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyz>0</iyz>
            </inertia>
        </inertial>

        <collision name="left_lower_leg_collision">
            <geometry>
                <box>
                    <size> 0.04 0.04 0.3 </size>
                </box>
            </geometry>
        </collision>

        <visual name="left_lower_leg_visual">
            <geometry>
                <box>
                    <size> 0.04 0.04 0.3 </size>
                </box>
            </geometry>
        </visual>
    </link>

    <joint type="fixed" name="left_foot_lower_leg_joint">
        <pose> 0 0 0 0 0 0 </pose>
        <parent>left_lower_leg</parent>
        <child>left_foot</child>
    </joint>

    <link name="left_upper_leg">
        <self_collide>1 </self_collide>
        <!--Height should be foot diameter 
            + lower leg link length
            + spacing for actuator between lower and upper leg
            + upper leg link length / 2-->
        <pose> 0.1 0 0.55 0 0 0 </pose>

        <inertial>
            <mass> 0.05 </mass>
            <inertia>
                <ixx>0.00038</ixx>
                <iyy>0.00038</iyy>
                <izz>0.000014</izz>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyz>0</iyz>
            </inertia>
        </inertial>

        <collision name="left_upper_leg_collision">
            <geometry>
                <box>
                    <size> 0.04 0.04 0.3 </size>
                </box>
            </geometry>
        </collision>

        <visual name="left_upper_leg_visual">
            <geometry>
                <box>
                    <size> 0.04 0.04 0.3 </size>
                </box>
            </geometry>
        </visual>
    </link>

    <joint type="revolute" name="left_lower_leg_left_upper_leg_joint">
        <pose>0 0 0.17 0 0 0 </pose>

        <parent>left_upper_leg</parent>
        <child>left_lower_leg</child>

        <axis>
            <xyz>1 0 0 </xyz>
        </axis>
    </joint>

    <!--Right leg-->

    <link name="right_foot">
        <self_collide>1</self_collide>

        <pose> -0.1 0 0.03 0 0 0 </pose>

        <inertial>
            <mass> 0.01 </mass>
            <inertia>
                <ixx>0.0000045</ixx>
                <iyy>0.0000045</iyy>
                <izz>0.0000045</izz>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyz>0</iyz>
            </inertia>
        </inertial>

        <collision name="right_foot_collision">
            <geometry>
                <sphere>
                    <radius> 0.03 </radius>
                </sphere>
            </geometry>
        </collision>

        <visual name="right_foot_visual">
            <geometry>
                <sphere>
                    <radius> 0.03 </radius>
                </sphere>
            </geometry>
        </visual>
    </link>

    <link name="right_lower_leg">
        <self_collide>1 </self_collide>
        <!--Height should be foot diameter 
            + lower leg link length / 2-->
        <pose> -0.1 0 0.21 0 0 0 </pose>

        <inertial>
            <mass> 0.05 </mass>
            <inertia>
                <ixx>0.00038</ixx>
                <iyy>0.00038</iyy>
                <izz>0.000014</izz>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyz>0</iyz>
            </inertia>
        </inertial>

        <collision name="right_lower_leg_collision">
            <geometry>
                <box>
                    <size> 0.04 0.04 0.3 </size>
                </box>
            </geometry>
        </collision>

        <visual name="right_lower_leg_visual">
            <geometry>
                <box>
                    <size> 0.04 0.04 0.3 </size>
                </box>
            </geometry>
        </visual>
    </link>

    <joint type="fixed" name="right_foot_lower_leg_joint">
        <pose> 0 0 0 0 0 0 </pose>
        <parent>right_lower_leg</parent>
        <child>right_foot</child>
    </joint>

    
</model>
</sdf>
